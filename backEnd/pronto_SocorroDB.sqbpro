    id_paciente INTEGER PRIMARY KEY AUTOINCREMENT,
    nome TEXT NOT NULL,
    cpf TEXT NOT NULL,
    data_nascimento TEXT,
    genero TEXT
);

CREATE TABLE ENDERECO (
    id_endereco INTEGER PRIMARY KEY AUTOINCREMENT,
    rua TEXT NOT NULL,
    bairro TEXT NOT NULL,
	cidade TEXT NOT NULL,
	estado TEXT NOT NULL
);

CREATE TABLE ENDERECO_PACIENTE (
    id_paciente INTEGER NOT NULL,
    id_endereco INTEGER NOT NULL,
    PRIMARY KEY (id_paciente, id_endereco),
    FOREIGN KEY (id_paciente) REFERENCES PACIENTE(id_paciente),
    FOREIGN KEY (id_endereco) REFERENCES ENDERECO(id_endereco)
);

CREATE TABLE RECEPCIONISTA (
    id_recepcionista INTEGER PRIMARY KEY AUTOINCREMENT,
    nome TEXT NOT NULL,
    cpf TEXT NOT NULL
);

CREATE TABLE MARCA_CONSULTA (
    id_consulta INTEGER PRIMARY KEY AUTOINCREMENT,
    id_paciente INTEGER NOT NULL,
    id_recepcionista INTEGER NOT NULL,
    data_consulta_marcada TEXT NOT NULL,
    FOREIGN KEY (id_paciente) REFERENCES PACIENTE(id_paciente),
    FOREIGN KEY (id_recepcionista) REFERENCES RECEPCIONISTA(id_recepcionista)
);

CREATE TABLE CLASSIFICACAO_RISCOS (
    id_classificacao_risco INTEGER PRIMARY KEY AUTOINCREMENT,
    nome_classificacao TEXT NOT NULL,
    cor TEXT NOT NULL
);

CREATE TABLE TRIAGEM (
    id_triagem INTEGER PRIMARY KEY AUTOINCREMENT,
    id_classificacao_risco INTEGER NOT NULL,
    sintomas TEXT NOT NULL,
    temperatura REAL,
    saturacao REAL,
    pressao_sanguinea REAL,
    data_triagem TEXT,
    FOREIGN KEY (id_classificacao_risco) REFERENCES CLASSIFICACAO_RISCOS(id_classificacao_risco)
);

CREATE TABLE ENFERMEIRO (
    id_enfermeiro INTEGER PRIMARY KEY AUTOINCREMENT,
    nome TEXT NOT NULL,
    coren TEXT NOT NULL
);

CREATE TABLE TRIAGEM_ENFERMEIRO (
    id_triagem INTEGER NOT NULL,
    id_enfermeiro INTEGER NOT NULL,
    PRIMARY KEY (id_triagem, id_enfermeiro),
    FOREIGN KEY (id_triagem) REFERENCES TRIAGEM(id_triagem),
    FOREIGN KEY (id_enfermeiro) REFERENCES ENFERMEIRO(id_enfermeiro)
);

CREATE TABLE MEDICO (
    id_medico INTEGER PRIMARY KEY AUTOINCREMENT,
    nome TEXT NOT NULL,
    crm TEXT NOT NULL
);

CREATE TABLE ATENDIMENTO (
    id_atendimento INTEGER PRIMARY KEY AUTOINCREMENT,
    id_paciente INTEGER NOT NULL,
    id_triagem INTEGER NOT NULL,
    id_medico INTEGER NOT NULL,
    data_atendimento TEXT NOT NULL,
    FOREIGN KEY (id_paciente) REFERENCES PACIENTE(id_paciente),
    FOREIGN KEY (id_triagem) REFERENCES TRIAGEM(id_triagem),
    FOREIGN KEY (id_medico) REFERENCES MEDICO(id_medico)
);

CREATE TABLE DIAGNOSTICO (
    id_atendimento INTEGER PRIMARY KEY,
    descricao_diagnostico TEXT NOT NULL,
    FOREIGN KEY (id_atendimento) REFERENCES ATENDIMENTO(id_atendimento)
);</sql><current_tab id="0"/></tab_sql></sqlb_project>
